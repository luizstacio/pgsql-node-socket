{
  "html": {
    "indent_char": " ",
    "indent_size": 2,
    "end_with_newline": false,
    "unformatted": ["a", "sub", "sup", "b", "i", "u", "pre"],
    "wrap_line_length": 0
  },
  "css": {
    "allowed_file_extensions": ["css"],
    "end_with_newline": false,
    "indent_char": " ",
    "indent_size": 2,
    "selector_separator": " ",
    "selector_separator_newline": true
  },
  "js": {
    "indent_with_tabs": false,
    "end_with_newline": false,
    "indent_size": 2,
    "indent_char": " "
  }
}
